<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>abc</servlet-name>
    <servlet-class>com.raj.Add</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>def</servlet-name>
    <servlet-class>com.raj.Display</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>def</servlet-name>
    <url-pattern>/display</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ghi</servlet-name>
    <servlet-class>com.raj.Edit</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ghi</servlet-name>
    <url-pattern>/edit</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>jkl</servlet-name>
    <servlet-class>com.raj.Delete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>jkl</servlet-name>
    <url-pattern>/delete</url-pattern>
  </servlet-mapping>

</web-app>
